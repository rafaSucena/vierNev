	id SMALLINT UNSIGNED NOT NULL AUTO_INCREMENT,
	figurecaption VARCHAR(100),
	background VARCHAR(24) NOT NULL,
	src VARCHAR(255) NOT NULL,
	alt VARCHAR(255) NOT NULL,
	rotate BOOLEAN NOT NULL
);






